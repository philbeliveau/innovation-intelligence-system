# FastAPI Backend Environment Variables
# Copy this file to .env and fill in your actual values

# OpenRouter API Configuration
OPENROUTER_API_KEY=sk-or-v1-your-api-key-here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
LLM_MODEL=anthropic/claude-sonnet-4.5

# Server Configuration
PORT=8000
HOST=0.0.0.0

# CORS Configuration (comma-separated list of additional allowed origins)
# Used for Vercel preview deployments - add specific URLs as needed
ALLOWED_ORIGINS=

# Vercel Blob (for downloading PDFs from frontend)
VERCEL_BLOB_READ_WRITE_TOKEN=vercel_blob_rw_your-token-here

# Optional: Log Level
LOG_LEVEL=INFO
